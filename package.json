{
  "name": "opensprint",
  "version": "0.0.1",
  "description": "Backend API's for opensprint",
  "main": "index.ts",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint . --ext .ts",
    "ts-node": "ts-node -T -P tsconfig.json -r tsconfig-paths/register",
    "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register src/index.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bullmq": "4.14.2",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "7.1.4",
    "mongoose": "8.0.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.2.0",
    "nodemon": "3.0.1",
    "openai": "^4.24.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "uuid": "^9.0.1"
  }
}
